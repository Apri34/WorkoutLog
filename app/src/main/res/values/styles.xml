        <item name="textColorEditText">@color/colorPrimaryThemeLight</item>
        <item name="textColorSpinnerItemEditable">@color/white</item>
        <item name="textColorSpinnerItemNotEditable">@color/colorPrimaryThemeLight</item>
        <item name="calenderHeaderTextColor">@color/colorPrimaryThemeLight</item>
        <item name="selectedTabTextColor">@color/white</item>
        <item name="historyItemColor">@color/colorPrimaryThemeLight</item>
        <item name="tabIndicatorColor">@color/white</item>
        <item name="colorSpinnerItemExerciseInRoutine">@color/color_spinner_exercises_theme_light</item>
        <item name="checkbox_background">@drawable/checkbox_background_theme_light</item>


        <item name="ic_expand_more_colorsecondary_18dp">@drawable/ic_expand_more_colorsecondary_18dp_theme_light</item>
        <item name="ic_navigate_before_colorprimary_36dp">@drawable/ic_navigate_before_colorprimary_36dp_theme_light</item>

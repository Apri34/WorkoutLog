        <item name="ic_navigate_next_colorprimary_36dp">@drawable/ic_navigate_next_colorprimary_36dp_theme_light</item>
        <item name="ic_navigate_next_colorprimary_48dp">@drawable/ic_navigate_next_colorprimary_48dp_theme_light</item>
        <item name="ic_pause_colorprimary_30dp">@drawable/ic_pause_colorprimary_30dp_theme_light</item>
        <item name="ic_play_arrow_colorprimary_30dp">@drawable/ic_play_arrow_colorprimary_30dp_theme_light</item>
        <item name="ic_remove_colorprimary_30dp">@drawable/ic_remove_colorprimary_30dp_theme_light</item>
        <item name="ic_replay_colorprimary_30dp">@drawable/ic_replay_colorprimary_30dp_theme_light</item>
    </style>
</resources>
